<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Healthy Relationships Week at Carnegie Mellon" %></title>
    <%= stylesheet_link_tag "normalize", "all" %>
    <%= javascript_include_tag  "all" %>
  </head>
  
  <body class="<%= page_classes %>">

    <nav>
      <ul>
        <li><%= link_to "Help Out",  "/helpout.html" %></li>
        <li><%= link_to "Resources",  "/resources.html" %></li>
        <li><%= link_to "Who We Are",  "/whoweare.html" %></li>
        <li><%= link_to "About",  "/about.html" %></li>
      </ul>
    </nav>

    <header>
      <h1>
        <a href="/index.html"><span>Alpha Chi Omega supports</span> Healthy Relationships Week</a>
      </h1>
    </header>

    <%= yield %>

  </body>
</html>